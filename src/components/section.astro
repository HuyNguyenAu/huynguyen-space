---
export type Props = {
  title: string;
};

const { title } = Astro.props;
---

<section>
  <div class="container py-24 mx-auto">
    <div class="flex flex-wrap w-full justify-center md:justify-start mb-20 px-5">
      <h1 class="text-3xl font-medium text-gray-900">
        {title}
      </h1>
    </div>
    <div class="px-5">
      <slot />
    </div>
  </div>
</section>
